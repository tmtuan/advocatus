<%= form_for @educator, html: { multipart: true } do |f| %>

    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, :class => 'form-control' %>
    </div>

    <div class="form-group">
      <%= f.label :expertise %>
      <%= f.text_area :expertise, :class => 'form-control' %>
    </div>

    <%= f.submit "Save", :class => 'btn btn-primary' %>

<% end %>